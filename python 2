class student:
  def read(self):
    self.name=input()
    self.rollno=int(input())
class mark(student):
  def readm(self):
    self.m1=int(input())
    self.m2=int(input())
    self.m3=int(input())
class sports(student):
  def reads(self):
    self.g=int(input())
class result(mark,sports):
  def total(self):
    self.tot=self.m1+self.m2+self.m3+self.g
  def print(self):
    print(self.name,self.rollno,self.tot)
obj=result()
obj.read()
obj.readm()
obj.reads()
obj.total()
obj.print()
